#!/bin/sh
Xilinxload 0 17 ${HOME}/udaq/bit/gr/bufch/bufch.bit -s

# changed on  10-Mar 1998 by Hidetomo Y.
# /h0/rcmds/initbufch_nfa == initbufch_nfa_b5
# 07-Apr 2001 by Hidetomo P. Yoshida 
# for more 5 memory modules
# /h0/rcmds/initbufch_nfa == initbufch_nfa_b6

# Set parameters for buffer changer
nfa 17 9 0 0
nfa 17 16 0 0x0010 0
# buf_size 5568
nfa 17 17 0 0x15c0 0 
nfa 17 17 1 0x15c0 0
nfa 17 17 2 0x15c0 0
nfa 17 17 3 0x15c0 0
nfa 17 17 4 0x15c0 0
nfa 17 17 5 0x15c0 0
nfa 17 17 6 0x15c0 0
nfa 17 17 7 0x15c0 0
#
nfa 17 0 0 0
nfa 17 1 0 0
nfa 17 1 1 0
nfa 17 1 2 0
nfa 17 1 3 0
nfa 17 1 4 0
nfa 17 1 5 0
nfa 17 1 6 0
nfa 17 1 7 0

